<?xml version="1.0" encoding="UTF-8"?>
<web-app >
  <display-name>Unit Dispatch </display-name>
  <welcome-file-list>
  <welcome-file>Login.jsp</welcome-file>
  </welcome-file-list>

  <servlet>
    <servlet-name>InsertDB</servlet-name>
    <servlet-class>InsertDB</servlet-class>
  </servlet>
   <servlet-mapping>
        <servlet-name>InsertDB</servlet-name>
        <url-pattern>/insrt</url-pattern>
    </servlet-mapping>

  <servlet>
    <servlet-name>HelloWorld</servlet-name>
    <servlet-class>HelloWorld</servlet-class>
  </servlet>
   <servlet-mapping>
        <servlet-name>HelloWorld</servlet-name>
        <url-pattern>/hw</url-pattern>
    </servlet-mapping>
</web-app>
